    <style name="Theme.Final_ShopThuCung.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.Final_ShopThuCung.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="neumorphirmcard">
        <item name="neumorph_cornerFamily">rounded</item>
        <item name="neumorph_cornerSize">10dp</item>
